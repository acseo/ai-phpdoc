{
    "name": "molbal/ai-phpdoc",
    "description": "Writes missing PHPDoc comments using GPT-3.",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Molbal\\AiPhpdoc\\": "src/"
        }
    },
    "authors": [
        {
            "name": "molbal",
            "email": "molbal@outlook.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "symfony/console": "^6",
        "openai-php/client": "^0.10.0",
        "symfony/http-client": "^7.1",
        "nyholm/psr7": "^1.8"
    },
    "bin": [
        "bin/aiphpdocs"
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
